{
  "name": "mtg-collection-optimizer",
  "version": "1.0.0",
  "description": "A comprehensive MTG collection management and deck optimization platform",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "npm run install-frontend && cd frontend && npm run build",
    "start": "python run.py",
    "backend": "python run.py",
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && pip install -r requirements.txt",
    "setup": "npm run install-frontend && npm run install-backend",
    "vercel-build": "cd frontend && npm install && npm run build"
  },
  "keywords": [
    "magic-the-gathering",
    "mtg",
    "collection-management",
    "deck-builder",
    "commander",
    "pricing",
    "cards"
  ],
  "author": "Ryan Gurley",
  "license": "BUSL-1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/gurleyryan/MTG-Deck-Optimizer.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint-config-next": "^15.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/testing-library__react": "^10.0.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.4.0"
  }
}
